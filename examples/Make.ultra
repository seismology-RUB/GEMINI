#----------------------------------------------------
#  Example: Ultra sonics
#---------------------------------------------------
gfdsv:
	gfdsvrkf -fmin 10000. -fmax 500000. -tlen 0.000256 -klim "1. 1." -ze 0. -xlen 0.001 -s 0 -fnod 500000. \
		-sig 5. -wmrg .1 -msd "0.01 1" -on temp/nodes.ultra -o temp/ultra plexiglas.nm
gfdsvseis:
	gfdsvrkfseis -f ssa -o temp/seisultra.ssa -e 30. -dt 5.e-7 -lp "5 250000." -hp "2 20000."  \
		      -gvw "0.5 1.e-4" info.ultra temp/ultra.sf
plotfk:
	plotGreenFKSpectra -z 0.001 -bg -4. temp/ultra.sf
plotgseis:
	plotSection temp/seisultra.ssa
#-----------------------------------------------------------------------------------------------------------
modes:
	flmodesSA -fmin 100000. -fmax 500000. -cmin 0.3 -cmax 1.41 -tlen 0.000512 -o temp/modesultra.sa \
		-p 0 -nmin 5 -wlr 8 plexiglas.nm
