#----------------------------------------------------
#  Example: regional seismology
#----------------------------------------------------
gfdsv:
	gfdsvrkf -rst -p 0 -tlen 512 -fmin 0.000 -fmax 0.2 -fnod 0.2 -klim "0.5 0.5" -msd "100000. 3" \
		-sig 5. -ze 0. -s 1 -o temp/gfregio -on temp/nodes.regio -xlen 5000. -nmin 2 -wmrg 0.1 ak135q.nm
gfdsvseis:
	gfdsvrkfseis -o temp/seisregio.sff -e 30. -dt 0.5 -lp "5 0.1" -hp "2 0.01"  \
		-gvw "2.0 100." -zs 45000. info.aeg3c temp/gfregio.sm
plotfk:
	plotGreenFKSpectra -z 10000. -bg -4. temp/gfregio.sm
plotgseis:
	plotSection temp/seisregio.sff
#-----------------------------------------------------------------------------------------------------------
modes:
	flmodesSA -fmin 0.005 -fmax 0.2 -cmin 2. -cmax 6. -tlen 512 -o temp/modesregio.sa \
		-p 0 -nmin 5 -wlr 8 ak135q.nm
modeseis:
	modeseisSA -o temp/mseisregio.sff -hp "2 0.01" -lp "5 0.1" -e 40. -gvw "2.0 100" -dt 0.5 info.aeg3c temp/modesregio.sa
plotdisp:
	plotFldispersion -t S temp/modesregio.sa
plotmodes:
	plotFlmodes -t S temp/modesregio.sa
plotmseis:
	plotSection temp/mseisregio.sff
#-------------------------------------------------------------------------------------------------
